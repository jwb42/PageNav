<Page x:Class="pageNav.startPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:pageNav"
      mc:Ignorable="d" 
      d:DesignHeight="350" d:DesignWidth="525"
      Title="startPage">

    <Grid Background="White">
        <StackPanel HorizontalAlignment="Left" Height="139" Margin="57,201,0,0" VerticalAlignment="Top" Width="163">
            <Button x:Name="signInBtn" Content="Sign In" HorizontalAlignment="Left" VerticalAlignment="Top" Width="153" Height="54" Click="signInBtn_Click"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="139" Margin="312,201,0,0" VerticalAlignment="Top" Width="182">
            <Button x:Name="signOutBtn" Content="SignOut" HorizontalAlignment="Left" VerticalAlignment="Top" Width="172" Height="54" Click="signOutBtn_Click"/>
        </StackPanel>

    </Grid>
</Page>
